<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Data primitives and extensions for .NET Core.</Description>
    <VersionPrefix>0.0.1</VersionPrefix>
    <Authors>Geek Learning;Cyprien Autexier;Adrien Siffermann</Authors>
    <TargetFrameworks>net452;netstandard1.3;netstandard2.0</TargetFrameworks>
    <AssemblyName>GeekLearning.Primitives</AssemblyName>
    <PackageId>GeekLearning.Primitives</PackageId>
  </PropertyGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'net452' ">
    <Reference Include="System" />
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.ValueTuple" Version="4.4.0" />
  </ItemGroup>

</Project>
